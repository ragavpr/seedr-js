{
  "name": "seedr-js",
  "version": "1.0.0",
  "description": "Unofficial Seedr API for Node.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragavpr/seedr-js.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "seedr",
    "seedrcc",
    "seedr-api"
  ],
  "scripts": {
    "build": "tsup src/index.ts --outDir dist --format esm --dts",
    "release": "bun run build && changeset publish",
    "lint": "tsc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "formdata-node": "^6.0.3",
    "fs-extra": "^11.3.0",
    "got": "^14.4.7"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/ragavpr"
  }
}
